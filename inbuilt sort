#include <iostream>
#include<algorithm>
using namespace std;
bool compare(int a,int b){
    cout<<"comparing a:"<<a<<"and b:"<<b;
    cout<<endl;
    return a<b;
}
int main() {
    int n;
    n=5;
    
    
    int a[5]={7,89,45,3,4};
    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
    cout<<endl;
     sort(a,a+n,compare);
       
     for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
     cout<<endl;
    return 0;
}


#include <iostream>
using namespace std;

int main() {
    
    int a[100][100];
    int r,c;
    cout<<"Enter no of rows and columns"<<endl;
    cin>>r>>c;
    int val=1;
    for(int row=0;row<r;r++){
        for(int col=0;col<c;col++){
            a[row][col]=val;
            val++;
        }
    }
      for(int row=0;row<r;r++){
        for(int col=0;col<c;col++){
            cout<<a[row][col]<<" ";
        }
        
    }
    return 0;
}

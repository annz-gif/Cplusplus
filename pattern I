#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
   int n =0;
   cin>>n;
   char alphabet='a';
   int count=0;
    
   for(int i=1;i<=n;i++){
      for(int j=0;j<=i;j++){
            count++;
       if(count%2==0){
           cout<<char(alphabet-32);
       }
       else{
           cout<<alphabet;
       }
       alphabet++;
      }
       cout<<endl;
   }
   }
       
  

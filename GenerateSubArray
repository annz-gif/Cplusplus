
#include <iostream>
using namespace std;

void GenerateSubArray(int a[],int n){
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            for(int k=i;k<=j;k++){
                cout<<"  "<<a[k];
            }
        }
    }
}
int main() {
    int a[]={2,73,55,4,88};
    int n=sizeof(a)/sizeof(int);
    GenerateSubArray(a,n);
      
   return 0;
}

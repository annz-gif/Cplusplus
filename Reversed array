#include <bits/stdc++.h>
using namespace std;


int main() {
    cout<<"enter no of elements";
   int n;
   cin>>n;
      int a[n];
      cout<<"Enter the array to be reversed"<<endl;
   for(int i=0;i<n;i++){
       cin>>a[i];
   }
   int s=0;
   int end=n-1;
   while(s<end){
       swap(a[s],a[end]);
       s++;
       end--;
   }

   for(int i=0;i<n;i++){
       cout<<"reversed array:"<<endl;
        cout<<a[i]<<"  ";
      
   }
    return 0;
}
